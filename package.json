{
	"name": "bigfu-design",
	"version": "1.0.3",
	"description": "this is a ui library for vue",
	"main": "index.js",
	"scripts": {
		"dev": "cross-env BUILD_ENV=dev RUN_TYPE=runtime webpack serve --config ./build/webpack.example.config.js",
		"build:lib": "cross-env BUILD_ENV=prod RUN_TYPE=build webpack --config ./build/webpack.library.config.js",
		"build": "npm run build:single&npm run build:module",
		"build:single": "cross-env BUILD_ENV=prod RUN_TYPE=build webpack --config ./build/webpack.single-lib.config.js",
		"build:module": "cross-env BUILD_ENV=prod RUN_TYPE=build webpack --config ./build/webpack.module-lib.config.js"
	},
	"repository": {
		"type": "git",
		"url": "git@gitee.com:an_interested_swordsman/bigfu-design.git"
	},
	"keywords": [
		"vue",
		"ui",
		"design"
	],
	"author": "bigfu",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.14.6",
		"@babel/plugin-transform-runtime": "^7.14.5",
		"@babel/preset-env": "^7.14.7",
		"@babel/preset-typescript": "^7.14.5",
		"@babel/runtime": "^7.14.6",
		"@vue/compiler-sfc": "^3.1.4",
		"autoprefixer": "^10.3.1",
		"babel-loader": "^8.2.2",
		"babel-plugin-component": "^1.1.1",
		"clean-webpack-plugin": "^4.0.0-alpha.0",
		"cross-env": "^7.0.3",
		"css-loader": "^5.2.6",
		"css-minimizer-webpack-plugin": "^3.0.2",
		"fs": "0.0.1-security",
		"html-webpack-plugin": "^5.3.2",
		"ignore-loader": "^0.1.2",
		"less": "^4.1.1",
		"less-loader": "^10.0.1",
		"mini-css-extract-plugin": "^2.1.0",
		"path-browserify": "^1.0.1",
		"postcss": "^8.3.5",
		"postcss-loader": "^6.1.1",
		"postcss-preset-env": "^6.7.0",
		"style-loader": "^3.1.0",
		"terser-webpack-plugin": "^5.1.4",
		"ts-loader": "^9.2.3",
		"typescript": "^4.3.5",
		"vue-loader": "^16.3.0",
		"webpack": "^5.41.1",
		"webpack-cli": "^4.7.2",
